    "@storybook/react-webpack5": "^9.1.4",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/node": "^18.16.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/uuid": "^9.0.4",
    "bootstrap": "^5.2.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-plugin-storybook": "^9.1.4",
    "gh-pages": "^5.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.4.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.61.1",
    "reactstrap": "^9.2.3",
    "release-it": "^19.0.4",
    "rollup": "^4.46.2",
    "rollup-plugin-import-css": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "shx": "^0.4.0",
    "storybook": "^9.1.4",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "webpack": "^5.101.0",
    "yalc": "^1.0.0-pre.53"
  },
