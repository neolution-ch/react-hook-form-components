    "material-ui-popup-state": "^5.3.6",
    "react-datepicker": "^8.0.0",
    "react-number-format": "^5.4.4",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@neolution-ch/eslint-config-neolution": "2.2.0",
